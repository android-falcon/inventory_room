<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#d1f4e3" />
            <stroke
                android:width="3dp"
                android:color="#64b8e2" />
            <corners
                android:radius="5dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#d1f4e3"
                android:endColor="#92d0b1"
                android:angle="90" />
            <!--<stroke-->
                <!--android:width="3dp"-->
                <!--android:color="#34abb5" />-->
            <corners
                android:radius="900dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>